              void vIom_SetGpioVal(UINT8 GPn,UINT8 Val)
              {
                 vScu_IomClkEn();
                 if(GPn<=GP7)
                 {
                     if(Val)
                   {
                      IOMP0DOUT|= (0x01<<GPn);
                   }
                   else
                   {
                    IOMP0DOUT&=(~(0x01<<GPn));
                   }
                 }
                 else if(7<GPn<16)
